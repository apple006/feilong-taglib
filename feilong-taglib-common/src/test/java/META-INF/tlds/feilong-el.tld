<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd" version="2.1">

	<tlib-version>1.0</tlib-version>
	<short-name>fl</short-name>
	<uri>http://java.feilong.com/tags-el</uri>
	 
	<function>
		<description>判断一个值是否在一个集合(或者可以被转成Iterator)当中.</description>
		<name>isContain</name>
		<function-class>com.feilong.core.util.ListUtil</function-class>
		<function-signature>String _isContainTag(java.lang.Object,java.lang.Object)</function-signature>
		<example>${fl:isContain("a,b,c","a")}</example>
	</function>
</taglib>